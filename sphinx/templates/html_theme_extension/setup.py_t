setup(
    name='sphinxcontrib-themes-{{ name }}',
    version='1.0',
    packages=find_packages(),
    include_package_data=True,
    entry_points="""
        [sphinx.html_themes]
        {{ name }} = sphinxcontrib.themes.{{ name }}
    """,
)
